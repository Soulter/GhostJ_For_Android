<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.soulter.goastjforandroid">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".ScrSettingActivity"
            android:label="@string/title_activity_scr_setting"></activity>
        <activity android:name=".ScreenshotActivity" />
        <activity android:name=".FileExplorerActivity" />
        <activity android:name=".MessageFilterField" />
        <activity android:name=".ClientsFieldActivity" />
        <activity android:name=".DiyOrderActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />

        <service
            android:name=".ConnService"
            android:enabled="true"
            android:exported="true" />
    </application>

</manifest>